<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>AnimoServlet</servlet-name>
        <servlet-class>cnam.nfe114.AnimoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AnimoLogin</servlet-name>
        <servlet-class>cnam.nfe114.AnimoLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Testdb</servlet-name>
        <servlet-class>cnam.nfe114.Testdb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CatalogueServlet</servlet-name>
        <servlet-class>cnam.nfe114.CatalogueServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>action</servlet-name>
        <servlet-class>org.apache.struts.action.ActionServlet</servlet-class>
        <init-param>
            <param-name>config</param-name>
            <param-value>/WEB-INF/struts-config.xml</param-value>
        </init-param>
        <init-param>
            <param-name>debug</param-name>
            <param-value>2</param-value>
        </init-param>
        <init-param>
            <param-name>detail</param-name>
            <param-value>2</param-value>
        </init-param>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>AnimoServlet</servlet-name>
        <url-pattern>/AnimoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AnimoLogin</servlet-name>
        <url-pattern>/AnimoLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Testdb</servlet-name>
        <url-pattern>/Testdb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CatalogueServlet</servlet-name>
        <url-pattern>/CatalogueServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>action</servlet-name>
        <url-pattern>*.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
